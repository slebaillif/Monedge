<h1>Create new tab</h1>
<%= form_for @changeset, tab_path(@conn, :create), fn f -> %>

<div class="form-group">
  <%= text_input f, :name, placeholder: "Name", class: "form-control" %>
  <%= error_tag f, :name %>
</div>

<div class="form-group">
  <%= text_input f, :currency, placeholder: "Currency", class: "form-control" %>
  <%= error_tag f, :currency %>
</div>

<%= submit "Create Tab", class: "btn btn-primary" %>
<% end %>
