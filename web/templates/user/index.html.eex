<h1>Listing Users</h1>

<table class="table">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Link</th>
  </tr>
  <%= for user <- @users do %>
  <tr>
    <td><%= user.firstName %></td>
    <td><%= user.lastName %></td>
    
    <td><%= link "details", to: user_path(@conn, :show, user.firstName)  %></td>
  </tr>
  <% end %>
</table>
